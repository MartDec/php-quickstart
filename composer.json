{
    "name": "timar/quickstart",
    "description": "Personnal framework based on Slim microframework",
    "require": {
        "slim/slim": "^4.7",
        "slim/psr7": "^1.3",
        "haydenpierce/class-finder": "^0.4.3",
        "ext-json": "*",
        "ext-pdo": "*",
        "illuminate/database": "^8.27",
        "symfony/dotenv": "^5.2",
        "lcobucci/jwt": "^4.1"
    },
    "require-dev": {
        "symfony/var-dumper": "^5.2",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "./src",
            "Database\\": "./database",
            "Tests\\":  "./tests"
        }
    }
}
